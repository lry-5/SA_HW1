.list-view-container .carrynews{
	display: block;
	margin-left: 27px;
	text-decoration: none;
	line-height: 27px;
	font-size: 14px;
	height: 31px;
	width: 100px;
	color: #252525;
	border: 1px solid #D2D2D2;
	position: relative;
	padding-left: 11px;
	border-radius: 8px;
}
.list-view-container .carrynews:hover{
	background-color: #595C5F;
	color: white;
}

