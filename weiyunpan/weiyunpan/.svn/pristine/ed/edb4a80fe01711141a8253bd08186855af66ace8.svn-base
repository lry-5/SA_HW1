@CHARSET "UTF-8";
* {
	margin: 0;
	padding: 0;
}

body {
	font-size: 12px;
	font-family: "微软雅黑";
	color: #666;
}

div {
	display: block;
}
img{
	border: 0;
}
ul li {
	list-style: none;
}

/
/**
 * main_con
 */

.main_con {
	height: 100%;
}

.main_con_top {
	height: 30px;
	display: block;
	position: relative;
	padding-left: 10px;
	line-height: 30px;
}

.wj_total {
	position: absolute;
	right: 250px;
	top: 0;
}

.list-view ul {
	position: relative;
	background-color: #F7F7F7;
	height: 43px;
	border-radius: 2px;
	line-height: 41px;
	overflow: hidden;
	color: #888;
	border: 2px solid #d2d2d2;
}

.col {
	float: left;
	border-left: 1px solid #fff;
	border-right: 1px solid #e5e5e5;
	text-indent: 10px;
}

.first-col {
	border-right: 1px solid #e5e5e5;
	border-left: 0;
	text-indent: 0;
}

.span_wjm {
	font-size: 13px;
	padding-left: 12px;
}

.last-col {
	border-right: 0;
}